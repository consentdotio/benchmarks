	"includes": {
		"backend": ["nodejs", "typescript"],
		"components": ["react", "javascript"]
	},
	"internationalization": {
		"detection": "browser",
		"stringLoading": "server"
	},
	"company": {
		"name": "c15t",
		"website": "https://c15t.com",
