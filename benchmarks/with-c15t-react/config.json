{
	"$schema": "./node_modules/@cookiebench/benchmark-schema/schema.json",
	"name": "with-c15t-react",
	"iterations": 20,
	"cookieBanner": {
		"selectors": [
			"[data-testid=\"cookie-banner-root\"]",
			".c15t-banner",
			"[data-c15t-banner]",
			".consent-banner",
			"[data-consent-banner]"
		],
		"serviceHosts": ["c15t.com", "consent.io"],
